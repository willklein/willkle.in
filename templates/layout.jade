doctype html
html(lang='en', xmlns='http://www.w3.org/1999/html')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')

    title Will Klein

    //if lt IE 9
    script(src='http://html5shim.googlecode.com/svn/trunk/html5.js')

    link(href='/css/main.css', rel='stylesheet')
    link(href='http://netdna.bootstrapcdn.com/font-awesome/3.1.1/css/font-awesome.css', rel='stylesheet')
    link(href='http://fonts.googleapis.com/css?family=ABeeZee', rel='stylesheet', type='text/css')

  body(data-spy='scroll', data-target='.subhead', data-offset='50')
    .container
      header#overview
        #profile
          h1
            a#nameLink(href='/')
              img#profileImg.img-rounded(src='/img/profile.jpg')
              | 
              | Will Klein
          .subnav
            ul.nav.nav-pills
              li
                a(target='_blank', href='https://github.com/willklein', title='Github')
                  i.icon-github-sign
              li
                a(target='_blank', href='https://twitter.com/willslab', title='Twitter')
                  i.icon-twitter-sign
              li
                a(target='_blank', href='https://plus.google.com/u/0/101735064507096591711', title='Google+')
                  i.icon-google-plus-sign
              li
                a(target='_blank', href='http://linkedin.com/in/willklein', title='LinkedIn')
                  i.icon-linkedin-sign
              li
                a(target='_blank', href='http://stackoverflow.com/users/1239485/will-klein', title='Stack Overflow')
                  i.icon-ok-sign
        aside
          !=contents['about.md'].html

      section#content
        block content

      section#footer
        block footer

    script(src='http://netdna.bootstrapcdn.com/jquery/jquery.min.js')
    script(src='/js/clicky.js')
    script(src='/js/ga.js')

    block pageScript